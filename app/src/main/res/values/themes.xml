<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ATMConsultoria" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/cor1</item>
        <item name="colorPrimaryVariant">@color/cor2</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/cor1</item>
        <item name="colorSecondaryVariant">@color/cor2</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="colorPrimaryDark">@color/cor2</item>
        <item name="colorAccent">@color/cor3</item>
        <item name="colorOnBackground">@color/white</item>


    </style>

    <style name="Theme.ATMConsultoria.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ATMConsultoria.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ATMConsultoria.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>